<h1>Articles!</h1>
<%= link_to "new article", new_article_path%>


<ul>
  <% @articles.each do |article| %>
  <li>
    <h2><%= article.title.capitalize %>  </h2>
  <p>article.text</p>
  <%= link_to "show", article_path(article.id) %>
  <%= link_to "edit", edit_article_path(article.id) %>
  </li>
  <% end %>
</ul>
